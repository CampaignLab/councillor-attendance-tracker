{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "rewrites": [{ "source": "/(.*)", "destination": "/" }],
    "functions": {
        "api/updateData": {
            "maxDuration": 600
        }
    },
    "crons": [
        {
            "path": "/api/updateData",
            "schedule": "0 0 1 * *"
        }
    ]
}
